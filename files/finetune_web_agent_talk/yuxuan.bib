  shorttitle = {{{psiTurk}}},
  author = {Gureckis, Todd M. and Martin, Jay and McDonnell, John and Rich, Alexander S. and Markant, Doug and Coenen, Anna and Halpern, David and Hamrick, Jessica B. and Chan, Patricia},
