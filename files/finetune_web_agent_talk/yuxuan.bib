@misc{chanScalingSyntheticData2024,
  title = {Scaling {{Synthetic Data Creation}} with 1,000,000,000 {{Personas}}},
  author = {Chan, Xin and Wang, Xiaoyang and Yu, Dian and Mi, Haitao and Yu, Dong},
  year = {2024},
